<h1>Listing scripts</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Commande</th>
    <th>Label</th>
    <th>Description</th>
    <th>Previous script</th>
    <th>Next script</th>
  </tr>

<% @scripts.each do |script| %>
  <tr>
    <td><%= script.id %></td>
    <td><%= script.cmd %></td>
    <td><%= script.label %></td>
    <td><%= script.description %></td>
    <td><%= script.previous_script_ids %></td>
    <td><%= script.next_script_ids %></td>
    <td><%= link_to 'Show', script %></td>
    <td><%= link_to 'Edit', edit_script_path(script) %></td>
    <td><%= link_to 'Destroy', script, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Script', new_script_path %>
